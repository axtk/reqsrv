{
  "name": "reqsrv",
  "version": "4.2.0",
  "description": "Utilities for typing web APIs",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "npm-run-all cleanup compile",
    "cleanup": "node -e \"require('node:fs').rmSync('dist', {force: true, recursive: true});\"",
    "compile": "npm-run-all compile:src compile:tests",
    "compile:src": "esbuild index.ts --bundle --outfile=dist/index.js --platform=neutral --minify",
    "compile:tests": "esbuild tests.ts --bundle --outfile=dist/tests.js --platform=neutral  --external:node-fetch",
    "prepublishOnly": "npm-run-all build test",
    "preversion": "npm-run-all build test",
    "test": "node dist/tests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axtk/reqsrv.git"
  },
  "keywords": [
    "typed api",
    "schema",
    "typed schema"
  ],
  "author": "axtk",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.9.0",
    "esbuild": "^0.24.0",
    "npm-run-all": "^4.1.5"
  }
}
