{
  "name": "reqsrv",
  "version": "4.6.4",
  "description": "Typed SDK factory for web APIs",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "npx npm-run-all cleanup compile",
    "cleanup": "node -e \"require('node:fs').rmSync('dist', {force: true, recursive: true});\"",
    "compile": "npx npm-run-all compile:src compile:tests",
    "compile:src": "npx esbuild index.ts --bundle --outfile=dist/index.js --platform=neutral",
    "compile:tests": "npx esbuild tests.ts --bundle --outfile=dist/tests.js --platform=neutral",
    "gh-pages": "npx ghstage --color-scheme=mediumslateblue --ymid=101149630 --jsorg --redirect=https://t8js.github.io/sdk-factory",
    "prepublishOnly": "npx npm-run-all build test gh-pages",
    "preversion": "npx npm-run-all typecheck build test",
    "test": "node dist/tests.js",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axtk/reqsrv.git"
  },
  "keywords": [
    "api",
    "sdk",
    "schema",
    "request",
    "typed api",
    "typed sdk",
    "typed schema"
  ],
  "author": "axtk",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.9.0",
    "typescript": "^5.9.2"
  }
}
