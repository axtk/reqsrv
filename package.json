{
  "name": "reqsrv",
  "version": "1.2.1",
  "description": "A lightweight zero-dependency container for typed web APIs",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "cross-env npm run cleanup && npm run compile",
    "cleanup": "node -e \"require('fs').rmSync('dist', {force: true, recursive: true});\"",
    "compile": "tsc",
    "prepublishOnly": "npm run build",
    "preversion": "cross-env npm run build && npm run test",
    "test": "node --experimental-specifier-resolution=node dist/tests.js",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axtk/reqsrv.git"
  },
  "keywords": [
    "request",
    "request service",
    "api",
    "typed api",
    "schema",
    "typed schema"
  ],
  "author": "axtk",
  "license": "ISC",
  "devDependencies": {
    "@types/node-fetch": "2.6.1",
    "cross-env": "7.0.3",
    "node-fetch": "3.2.5",
    "typescript": "4.7.3"
  }
}
